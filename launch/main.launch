<launch>
  <node pkg="robot_driver" type="robot_driver" name="robot_driver" clear_params="true" output="screen">
    <param name="port" value="/dev/ttyUSB0" type="str" />
    <param name="baud_rate" value="115200" type="int" />
  </node>

  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_se" clear_params="true" output="screen">
    <rosparam command="load" file="../params/ekf_template.yaml" />
  </node>

  <node pkg="xv_11_laser_driver" type="neato_laser_publisher" name="xv_11_laser_driver" clear_params="true" output="screen">
  </node>

  <node pkg="motion_path_creator" type="motion_path_creator" name="motion_path_creator" clear_params="true" output="screen">
  </node>

  <node pkg="tf" type="static_transform_publisher" name="lidar_tf_broadcaster" args="0 0 0 -0.5 -0.5 0.5 -0.5 base_link neato_laser 50" />

  <node pkg="tf" type="static_transform_publisher" name="field_odom_broadcaster" args="0.9144 0.3048 0 1.5707963 0 0 field odom 5" />


  <!--
    <node pkg="goat_lidar_merging" type="goat_lidar_merging" name="goat_lidar_merging" clear_params="true">
    </node>
  -->
</launch>
